        def enterPress():
            value_1 = int(self.entry_1.text())
            digit = 0
            if value_1 > 0 :
                #number of digits
                value_1_1 = len(str(value_1))
                #tens digit
                value_1_2 = int(int(value_1) / (10 ** (value_1_1 - 1)))
                #first two digits
                value_1_3 = int(int(value_1) / (10 ** (value_1_1 - 2)))
                #raunded value
                value_1_4 = int(int(value_1_3) * (10 ** (value_1_1 - 2)))
                #ones digits
                value_1_5 = int(value_1_3 - (value_1_2 * 10))

                self.label_1.setText(str(value_1_4))

                self.entry_1.setText(str(value_1_4))

                print(value_1_2)
                if int(value_1_1) > 1:
                    print(str(int(value_1_5)))

                    if int(value_1_5) == 0:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: black;"
                                                    "border-color: black;");
                    if int(value_1_5) == 1:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #964b00;"
                                                    "border-color: black;");
                    if int(value_1_5) == 2:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #ff2400;"
                                                    "border-color: black;");
                    if int(value_1_5) == 3:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #FF7F00;"
                                                    "border-color: black;");
                    if int(value_1_5) == 4:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #ffff00 ;"
                                                    "border-color: black;");
                    if int(value_1_5) == 5:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #03c03c;"
                                                    "border-color: black;");
                    if int(value_1_5) == 6:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #002fa7;"
                                                    "border-color: black;");
                    if int(value_1_5) == 7:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #660099;"
                                                    "border-color: black;");
                    if int(value_1_5) == 8:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #908f8f;"
                                                    "border-color: black;");
