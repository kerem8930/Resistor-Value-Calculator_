            self.slider_3.setStyleSheet("border:none ;");
            self.slider_3.setGeometry(132,388,125,30)
            self.slider_3.setOrientation(Qt.Horizontal)
            self.slider_3.setTickPosition(QSlider.TicksBelow)
            self.slider_3.setTickInterval(1)
            self.slider_3.setMinimum(0)
            self.slider_3.setMaximum(8)
            self.slider_3.valueChanged.connect(sliderValue)

        def sliderValue():
            c3_1 =int(self.slider_1.value())
            c2_1 =int(self.slider_2.value())
            c1_1 =int(self.slider_3.value())

            if c3_1 < 9:
                value_2 = int(((c1_1 + 1)*10 + c2_1) * (10 ** c3_1))
            else:
                value_2 = int(((c1_1 + 1)*10 + c2_1) * 0.1)

            self.label_1.setText(str(value_2))

            self.entry_1.setText(str(value_2))

            enterPress()

