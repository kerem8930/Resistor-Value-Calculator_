            self.slider_1 = QSlider(self)
            self.slider_1.setStyleSheet("border:none ;");
            self.slider_1.setGeometry(132,488,125,30)
            self.slider_1.setOrientation(Qt.Horizontal)
            self.slider_1.setTickPosition(QSlider.TicksBelow)
