                                                    "border-width: 1px;"
                                                    "background-color: #660099;"
                                                    "border-color: black;");
                    if int(value_1_2) == 8:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #908f8f;"
                                                    "border-color: black;");
                    if int(value_1_2) == 9:
                        self.button_4.setStyleSheet("border-style: outset;"
                                                    "border-width: 1px;"
                                                    "background-color: #ffffff;"
                                                    "border-color: black;");

            else:
                msg = QMessageBox()
                msg.setIcon(QMessageBox.Critical)
                msg.setText("Error")
                msg.setInformativeText('Please enter positive value')
                msg.setWindowTitle("Error")
                msg.exec_()


        part_2_1()
